{
  "expo": {
    "name": "Aurora",
    "slug": "aurora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aurora_Logo.png",
    "scheme": "aurora",
    "backgroundColor": "#0B0B0B",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/aurora_Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B0B0B"
      }
    },
    "splash": {
      "image": "./assets/images/aurora_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0B"
    },
    "android": {
      "backgroundColor": "#0B0B0B",
      "splash": {
      "image": "./assets/images/aurora_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0B"
    },
      "navigationBar": {
        "backgroundColor": "#0B0B0B",
        "barStyle": "light-content"
      },
      "statusBar": {
        "backgroundColor": "#0B0B0B",
        "barStyle": "light-content"
      },
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/aurora_Logo.png",
        "backgroundColor": "#0B0B0B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.frontend"
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/aurora_Logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/aurora_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0B0B0B"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "API_URL": "http://192.168.0.10:5270"
    }
  }
}
